{
    "name": "headless-wp-forms/contact-form-7",
    "description": "",
    "license": "UNLICENSE",
    "require": {
        "php": "^8.2",
        "headless-wp-forms/serverless-mocked-response": "*"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.47",
        "phpat/phpat": "^0.12.1",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.0",
        "rector/swiss-knife": "^2.2",
        "slevomat/coding-standard": "^8.18",
        "squizlabs/php_codesniffer": "^3.13",
        "phpstan/extension-installer": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "HeadlessWpForms\\ContactForm7\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeadlessWpForms\\ContactForm7\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../lib/*",
            "canonical": true
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "lock": false
    },
    "scripts": {
        "phpcs": "@php ./vendor/bin/phpcs",
        "phpcs:fix": "@php ./vendor/bin/phpcbf",
        "phpstan": "@php ./vendor/bin/phpstan",
        "phpunit": "@php ./vendor/bin/phpunit",
        "rector": "@php ./vendor/bin/rector",
        "fix": [
            "@phpcs:fix",
            "@rector"
        ],
        "qa": [
            "@phpcs",
            "@phpstan",
            "@phpunit"
        ]
    }
}
