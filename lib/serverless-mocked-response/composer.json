{
    "name": "headless-wp-forms/serverless-mocked-response",
    "description": "",
    "license": "UNLICENSE",
    "require": {
        "php": "^8.2",
        "kekos/multipart-form-data-parser": "^1.1",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.47",
        "phpat/phpat": "^0.12.1",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^2.0",
        "rector/swiss-knife": "^2.2",
        "slevomat/coding-standard": "^8.18",
        "squizlabs/php_codesniffer": "^3.13"
    },
    "autoload": {
        "psr-4": {
            "HeadlessWpForms\\ServerlessMockedResponse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HeadlessWpForms\\ServerlessMockedResponse\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true
        },
        "lock": false
    },
    "scripts": {
        "fix": [
            "@phpcs:fix",
            "@rector"
        ],
        "phpcs": "@php ./vendor/bin/phpcs",
        "phpcs:fix": "@php ./vendor/bin/phpcbf",
        "phpstan": "@php ./vendor/bin/phpstan",
        "phpunit": "@php ./vendor/bin/phpunit",
        "qa": [
            "@phpcs",
            "@phpstan",
            "@phpunit"
        ],
        "rector": "@php ./vendor/bin/rector"
    }
}
